<h1>Song List</h1>
<%= link_to "New song", new_song_path %>

<table id="song_list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Release Year</th>
    </tr>
  </thead>
  
  <tbody>
    <% @songs.each do |song| %>
        <tr>
          <td><%= link_to song.title, song_path(song) %></td>
          <td><%= song.artist_name %></td>
          <td<%= song.release_year %>></td>
        </tr>
    <% end %>
  </tbody>
</table>
